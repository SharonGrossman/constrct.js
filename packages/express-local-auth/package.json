{
  "name": "express-local-auth",
  "version": "0.0.0",
  "description": "Express local auth handler",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "scripts": {
    "babel": "babel src -d dist -s",
    "clean": "rimraf dist",
    "precommit": "lint-staged",
    "copy": "copyfiles package.json .env.defaults .env.schema dist",
    "build": "yarn clean && yarn babel && yarn copy",
    "lint": "eslint src --fix",
    "start": "yarn build && node dist/app.js",
    "dev": "nodemon"
  },
  "keywords": [
    "jwt",
    "middleware",
    "express"
  ],
  "author": "Sharon Grossman",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.6.2",
    "@babel/core": "^7.6.2",
    "@babel/node": "^7.6.2",
    "@babel/preset-env": "^7.6.2",
    "copyfiles": "^2.1.1",
    "nodemon": "^1.19.3"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "dotenv-extended": "^2.4.0",
    "env-bunyan": "^0.1.0",
    "express": "^4.17.1",
    "express-auth": "^0.0.0",
    "express-json-error-handler": "^3.0.0",
    "helmet": "^3.21.1",
    "in-production": "^1.0.1",
    "jsonwebtoken": "^8.5.1",
    "method-override": "^3.0.0",
    "models": "^0.0.0",
    "mongoose": "^5.7.1",
    "morgan": "^1.9.1",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0"
  },
  "private": true
